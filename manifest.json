{
  "name": "Dark mode",
  "version": "1.0.0",
  "description": "force dark mode",
  "manifest_version": 2,
  "content_scripts":[
      {
          "matches":["<all_urls>"],
          "js":["script.js"],
          "css":["styles.css"]
      }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Dark mode"
  }
}
